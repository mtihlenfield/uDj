<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel='stylesheet' href='/css/normalize.css'>
    <link rel='stylesheet' href='/css/style.css'>
    <link href="https://fonts.googleapis.com/css?family=Alegreya+Sans+SC:300,400" rel="stylesheet">
    <link rel="icon" type="image/png" href="img/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="img/favicon-16x16.png" sizes="16x16" />
    <script async src="/js/jquery.min.js"></script>
    <script async src="/js/main.js"></script>
  </head>
  <body>
    <div class="header shadow">
      <h1><%= title %></h1>
    </div>
      <div class="record-box">
        <div class="record shadow">
          <div class="ring-container spin">
            <div class="ring">
            </div>
            <div class="ring2">
            </div>
            <div class="ring3">
            </div>
            <div class="ring4">
            </div>
            <div class="ring5">
            </div>
            <div class="ring6">
            </div>
            <div class="ring7">
            </div>
            <div class="inner">
            </div>
            <div class="dot">
            </div>
           </div>
        </div>
      </div>
      <div class="section controls">
        <img id="play" src="img/play.png" alt="play" >
        <img id="pause" src="img/pause.png" alt="pause" class="hidden">
        <img id="skip" src="img/skip.png" alt="skip">
      </div>
    <div class="section">
      <div class="section current-song">
         <h4></h4>
      </div>
    </div>
    <div class="section add-btn">
      <button type="button" id="add-song-btn">Add Song</button>
    </div>
    <div class="section add-song">
      <h3>Artists</h3>
      <ul class="artist-list">
      </ul>
    </div>
    <div id="queue" class="section">
      <h3>Queue</h3>
      <div class="queue-list">

      </div>
    </div>

  </body>
</html>
